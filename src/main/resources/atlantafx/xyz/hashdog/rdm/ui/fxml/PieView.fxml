<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.shape.Circle?>

<AnchorPane prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="atlantafx.xyz.hashdog.rdm.ui.controller.PieController">
   <children>
      <StackPane fx:id="stackPane" style="-fx-background-color: #1e90ff;">
         <children>
            <PieChart fx:id="pie" />
            <Circle fx:id="circle" fill="DODGERBLUE" radius="100.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" />
         </children>
      </StackPane>
   </children>
</AnchorPane>
